<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#E1FFFC">

    <TextView
        android:id="@+id/tvTitle"
        android:layout_width="117dp"
        android:layout_height="19dp"
        android:layout_marginStart="1dp"
        android:background="#9CC19D"
        android:text="TextView"
        android:textColor="@color/wtite_midell"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvNote"
        android:layout_width="129dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:background="#9CC19D"
        android:inputType="text"
        android:text="TextView"
        android:textColor="#062201"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/tvTitle"
        app:layout_constraintStart_toEndOf="@+id/tvTitle"
        app:layout_constraintTop_toTopOf="@+id/tvTitle"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="21dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="23dp" />

    <TextView
        android:id="@+id/tvUri"
        android:layout_width="144dp"
        android:layout_height="19dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="8dp"
        android:background="#9CC19D"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/tvNote"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvNote"
        app:layout_constraintTop_toTopOf="@+id/tvNote"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="12dp"
        android:layout_height="19dp"
        android:background="#A8C1A9"
        app:layout_constraintBottom_toBottomOf="@+id/tvTitle"
        app:layout_constraintTop_toTopOf="@+id/tvTitle"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="-6dp" />
</androidx.constraintlayout.widget.ConstraintLayout>